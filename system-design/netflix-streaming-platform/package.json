{
  "name": "netflix-streaming-platform",
  "version": "1.0.0",
  "description": "Production-ready Netflix-like video streaming platform",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "setup-db": "node scripts/setup-db.js",
    "migrate": "psql -U postgres -d netflix_db -f src/db/schema.sql",
    "check-aws": "ts-node scripts/check-aws-config.ts",
    "test-s3": "ts-node scripts/test-s3-upload.ts",
    "seed": "ts-node scripts/seed-content.ts"
  },
  "keywords": [
    "netflix",
    "video-streaming",
    "hls",
    "dash",
    "typescript",
    "express",
    "postgresql",
    "redis",
    "elasticsearch",
    "s3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "@aws-sdk/client-s3": "^3.490.0",
    "validator": "^13.11.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "@elastic/elasticsearch": "^8.11.0",
    "cassandra-driver": "^4.7.2",
    "fluent-ffmpeg": "^2.1.2",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/validator": "^13.11.7",
    "@types/cors": "^2.8.19",
    "@types/helmet": "^0.0.48",
