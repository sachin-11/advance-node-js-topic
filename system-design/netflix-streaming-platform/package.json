    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^9.0.7",
    "@types/express-validator": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/fs-extra": "^11.0.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
